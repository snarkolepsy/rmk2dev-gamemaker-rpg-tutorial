            <kind>1</kind>
            <string>/// Handle Input Logic

// movement is mapped to WASD
if(keyboard_check(ord('A'))) {
    sprite_index = spr_Hero_Left;
    phy_position_x -= 2 * walk_speed;
}

if(keyboard_check(ord('D'))) {
    sprite_index = spr_Hero_Right;
    phy_position_x += 2 * walk_speed;
}

if(keyboard_check(ord('W'))) {
    sprite_index = spr_Hero_Up;
    phy_position_y -= 2 * walk_speed;
}

if(keyboard_check(ord('S'))) {
    sprite_index = spr_Hero_Down;
    phy_position_y += 2 * walk_speed;
}

// use shift key to adjust movement speed
if (keyboard_check(vk_shift)) {
    // Interaction
    walk_speed = lerp(walk_speed, 3.5, 0.1);
}
else {
    walk_speed = lerp(walk_speed, 1.0, 0.1);
}

// use/interact key
if (keyboard_check(ord('E'))) { // key is pressed
    action = true;
}
else { // when key is released
    action = false;
}
</string>
