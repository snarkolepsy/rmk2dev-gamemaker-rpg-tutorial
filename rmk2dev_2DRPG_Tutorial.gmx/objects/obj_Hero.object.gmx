            <string>/// spawns the attack type

// direction from player to mouse
dir = point_direction(phy_position_x, phy_position_y, mouse_x, mouse_y);

spawn_x = lengthdir_x(16, dir);
spawn_y = lengthdir_y(16, dir);

if(attack_type == 1) { // attack with sword - MELEE

    // creates an instance of the Sword Swing
    attack = instance_create(phy_position_x + spawn_x, phy_position_y + spawn_y, obj_Sword_Swing);

    with (attack) {
        phy_rotation = -other.dir;
    }
}
else if(attack_type == 2) { // attack with fireball - RANGED

    // creates an instance of the fireball  
    attack = instance_create(phy_position_x + spawn_x, phy_position_y + spawn_y, obj_Fireball);

    with (attack) {
        phy_rotation = -other.dir;
        physics_apply_impulse(x, y, other.spawn_x, other.spawn_y);
    }
}
</string>
